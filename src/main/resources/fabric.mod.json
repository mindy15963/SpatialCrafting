{
  "schemaVersion": 1,
  "id": "spatialcrafting",
  "version": "${version}",
  "name": "Spatial Crafting",
  "description": "3 dimensional crafters at varying sizes.",
  "authors": [
    "Fudge"
  ],
  "contact": {
    "homepage": "https://github.com/natanfudge/SpatialCrafting",
    "sources": "https://github.com/natanfudge/SpatialCrafting"
  },
  "license": "The MIT License",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "spatialcrafting.SpatialCraftingKt::init"
      }
    ],
    "client": [
      {
        "value": "spatialcrafting.SpatialCraftingKt::initClient"
      }
    ],
    "rei_plugins": [
      "spatialcrafting.compat.rei.ReiSpatialCraftingPlugin"
    ]
  },
  "mixins":[
   "spatialcrafting.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.3.50"
  },
  "recommends": {
    "roughly-enough-items": "*",
    "data-loader" : "*"
  }
}